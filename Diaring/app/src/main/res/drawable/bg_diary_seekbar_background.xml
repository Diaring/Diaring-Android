<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--시크바 배경-->
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <stroke android:width="11dp" android:color="@color/transparency_0" />
            <solid android:color="@color/transparency_75"/>
            <corners android:radius="10dp"/>
            <size android:height="10dp"/>
        </shape>
    </item>
    <!-- 시크바 배경2 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="6dp" android:color="@color/transparency_50" />
            </shape>
        </clip>
    </item>
    <!-- 시크바 프로그래스 -->
    <item android:id="@android:id/widget_frame" >
        <clip>
            <shape android:shape="rectangle">    <!--선 : 길이6dp -->
                <stroke android:width="11dp" android:color="@color/transparency_0" />
                <solid android:color="@color/white"/>
                <corners android:radius="10dp"/>
                <size android:height="10dp"/>
            </shape>
        </clip>
    </item>
</layer-list>